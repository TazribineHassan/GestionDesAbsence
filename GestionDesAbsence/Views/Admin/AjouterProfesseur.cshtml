@{
    ViewBag.Title = "AjouterProfesseur";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="container-fluid">
    <br />
    <h2>Ajouter Les Professeurs </h2>
    <br />
    <h4>Via Excel File </h4>
    <img src="/dist/img/prof.png" alt="User Avatar" class="img-size-50 mr-3 img-circle">
    <hr />

    @using (Html.BeginForm("AddProfs", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <h4>
            <img src="/dist/img/excel.png" alt="User Avatar" class="img-size-50 mr-3 img-circle" />
            Sélectionez un fichier
        </h4>
        <input name="file" type="file" id="file" /><br /><br />
        <input type="submit" id="fileup" class="btn btn-primary" data-toggle="modal" value="submit" />
        <br /><br />

    }

    <a href="/Admin/AllProfesseur"> Consulter la liste des professeurs</a>


</div>